@model IEnumerable<string>

@Html.ActionLink("Домой", "List", "Dresses", null, new { @class = "btn btn-block btn-default btn-lg" })

@foreach (var link in Model)
{
    @Html.RouteLink(link, new
    {
        controller = "Dresses",
        action = "List",
        category = link,
        page = 1
    }, new
    {
        @class = "btn btn-block btn-default btn-lg"
         + (link == ViewBag.CurrentCategory ? " btn-primary" : "")
    })
}